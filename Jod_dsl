job('Maven complie') {
 scm {
      git('https://github.com/veenagit123/Java_poject_sample.git')
      }
      
    
steps{
   maven{
            goals('clean install')
            goals('verify')
            mavenOpts('-Xms256m')
            mavenOpts('-Xmx512m')
            localRepository(LocalRepositoryLocation.LOCAL_TO_WORKSPACE)
            properties(skipTests: true)
            mavenInstallation('Maven 3.1.1')
            providedSettings('central-mirror')
   
   }
  
}
}
